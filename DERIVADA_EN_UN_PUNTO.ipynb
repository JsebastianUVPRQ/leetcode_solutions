    "# crea una funcion que calcule la derivada de una funcion en un punto x_0 = a\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "def derivada(f: callable, a, h=1e-5):\n",
    "    \n",
    "    return pd.Series({'f\\'(a)': (f(a+h) - f(a-h))/(2*h)})\n",
    "\n",
    "f = lambda x :5*x - (x**2)/4 + 3/(x-1)\n",
    "derivada(f, -3)\n"
   ]
  }
